<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from CalculatorServiceService WSDL service -->

  <service name="CalculatorService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://service.soap.metacube.com"/>
      <parameter name="wsdlServiceElement" value="CalculatorServiceService"/>
      <parameter name="schemaQualified" value="http://service.soap.metacube.com"/>
      <parameter name="wsdlServicePort" value="CalculatorService"/>
      <parameter name="className" value="com.metacube.soap.service.CalculatorServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="CalculatorService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="multiply" qname="operNS:multiply" xmlns:operNS="http://service.soap.metacube.com" returnQName="retNS:multiplyReturn" xmlns:retNS="http://service.soap.metacube.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:a" xmlns:pns="http://service.soap.metacube.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:b" xmlns:pns="http://service.soap.metacube.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="area" qname="operNS:area" xmlns:operNS="http://service.soap.metacube.com" returnQName="retNS:areaReturn" xmlns:retNS="http://service.soap.metacube.com" returnType="rtns:float" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:a" xmlns:pns="http://service.soap.metacube.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:b" xmlns:pns="http://service.soap.metacube.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:c" xmlns:pns="http://service.soap.metacube.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="sayHello" qname="operNS:sayHello" xmlns:operNS="http://service.soap.metacube.com" returnQName="retNS:sayHelloReturn" xmlns:retNS="http://service.soap.metacube.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="addition" qname="operNS:addition" xmlns:operNS="http://service.soap.metacube.com" returnQName="retNS:additionReturn" xmlns:retNS="http://service.soap.metacube.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:a" xmlns:pns="http://service.soap.metacube.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:b" xmlns:pns="http://service.soap.metacube.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="area multiply addition sayHello"/>

  </service>
</deployment>
